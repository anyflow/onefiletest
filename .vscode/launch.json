{
  "version": "0.2.0",
  "configurations": [{
      "name": "C++",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/bin/cpp/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": true,
      "logging": {
        "programOutput": true,
        "trace": true
      },
      "launchCompleteCommand": "exec-run",
      "linux": {
        "MIMode": "gdb",
        "setupCommands": [{
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }]
      },
      "osx": {
        "MIMode": "lldb",
        "setupCommands": [{
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }]
      },
      "windows": {
        "MIMode": "gdb",
        "setupCommands": [{
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }]
      }
    },
    {
      "name": "Java",
      "type": "java",
      "request": "launch",
      "stopOnEntry": true,
      "cwd": "${fileDirname}",
      "startupClass": "${fileBasename}",
      "sourcePath": [
        "${workspaceRoot}/java"
      ],
      "jdkPath": "/usr/bin",
      "classpath": [
        "${workspaceRoot}/bin/java"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "javascript",
      "program": "${file}",
      "cwd": "${workspaceRoot}/javascript",
      "env": {}
    },
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "stopOnEntry": true,
      "pythonPath": "${config:python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "env": {},
      "envFile": "${workspaceRoot}/.env",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ]
    }
  ]
}