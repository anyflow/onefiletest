{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "javascript",
      "program": "${file}",
      "cwd": "${workspaceRoot}/javascript",
      "env": {}
    },
    {
      "name": "Java",
      "type": "java",
      "request": "launch",
      "stopOnEntry": true,
      "cwd": "${fileDirname}",
      "startupClass": "${workspaceRoot}/java/${fileBasename}",
      "jdkPath": "/usr/bin",
      "options": [
      ]
    }
  ]
}